
# AWS Configuration (使用 IAM Role 或 EC2 Instance Profile，无需配置 AK/SK)
AWS_REGION=us-west-2
AWS_ACCOUNT_ID=123456789012

# 可选: 如果不使用 IAM Role，可以手动配置 AK/SK
# AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
# AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# Login credentials
LOGIN_USERNAME=admin
LOGIN_PASSWORD=password123
LOGIN_ENABLE=true

# AgentCore Runtime Configuration - Direct Code Deployment
DEPLOYMENT_PACKAGE_PATH=deployment_packages/strands_agent/deployment_package.zip
S3_BUCKET=bedrock-agentcore-code-123456789012-us-west-2
EXECUTION_ROLE_ARN=arn:aws:iam::123456789012:role/AmazonBedrockAgentCoreSDKRuntime-us-west-2

# AgentCore Runtime Configuration - Container Deployment
# 注意：运行前需要预先构建并推送 Docker 镜像到 ECR
# 构建命令参考: docker buildx build --platform linux/arm64 -t {ACCOUNT_ID}.dkr.ecr.{REGION}.amazonaws.com/{REPOSITORY}:{TAG} --push .
CONTAINER_ECR_REPOSITORY_NAME=my-strands-agent
CONTAINER_IMAGE_TAG=latest
CONTAINER_EXECUTION_ROLE_ARN=arn:aws:iam::123456789012:role/AmazonBedrockAgentCoreContainerRuntime-us-west-2